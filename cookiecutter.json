{
  "project_name": "Python Boilerplate",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "project_dash_slug": "{{ cookiecutter.project_slug.lower().replace('_', '-') }}",
  "github_repo": "syapse/{{ cookiecutter.project_dash_slug }}",
  "project_short_description": "Python Boilerplate contains all the boilerplate you need to create a Python package.",
  "version": "0.1.0",
  "command_line_interface": ["Click", "No command-line interface"],
  "enable_packagecloud": ["yes", "no"],
  "packagecloud_repo": "{% if cookiecutter.enable_packagecloud | lower == 'yes' %}syapse/General{% else %}DISABLED{% endif %}",
  "packagecloud_read_token": "{% if cookiecutter.enable_packagecloud | lower == 'no' %}DISABLED{% endif %}"
}
